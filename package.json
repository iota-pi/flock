{
  "name": "flock",
  "type": "module",
  "version": "1.0.0",
  "description": "Flock app and vault API",
  "author": "David Adams",
  "license": "MIT",
  "private": true,
  "scripts": {
    "analyse": "source-map-explorer 'dist/static/js/*.js'",
    "build": "tsc && vite build",
    "build:vault": "esbuild src/vault/index.ts --outfile=dist/vault/lambda.js --bundle --minify --platform=node --target=node22 --external:\"@aws-sdk/*\" && cd dist/vault && bestzip lambda.zip lambda.js",
    "deploy": "sst deploy",
    "deploy:app": "aws s3 sync dist/ s3://$APP_BUCKET/ --delete --cache-control max-age=$CACHE_MAX_AGE",
    "dev": "vite",
    "sst": "sst dev",
    "dev:vault": "tsx watch src/vault/api/runServer.ts",
    "initdb": "tsx src/vault/drivers/init.ts",
    "lint": "eslint src/ --ext .ts,.tsx",
    "start": "docker compose up -d && concurrently -n \"APP,API\" -c \"cyan,magenta\" \"yarn dev\" \"yarn dev:vault\"",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "e2e": "cypress run"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.513.0",
    "@aws-sdk/core": "^3.513.0",
    "@aws-sdk/lib-dynamodb": "^3.513.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fastify/auth": "^5.0.3",
    "@fastify/aws-lambda": "^6.1.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/type-provider-typebox": "^6.1.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/lab": "^7.0.1-beta.20",
    "@mui/material": "^7.3.6",
    "@mui/types": "^7.2.11",
    "@mui/x-date-pickers": "^8.23.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/query-async-storage-persister": "^5.90.14",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-persist-client": "^5.90.14",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "axios": "^1.13.2",
    "csv-parse": "^5.5.5",
    "date-fns": "^4.1.0",
    "esbuild": "^0.27.0",
    "fastify": "^5.6.2",
    "firebase": "^10.7.1",
    "firebase-admin": "^10.0.0",
    "happy-dom": "^20.0.10",
    "js-file-download": "^0.4.12",
    "match-sorter": "^6.3.1",
    "mui-file-dropzone": "^4.0.2",
    "p-map": "^7.0.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-icons": "^4.2.0",
    "react-redux": "^9.0.4",
    "react-router": "^7.9.6",
    "react-window": "^2.2.3",
    "tsx": "^4.20.6",
    "typebox": "^1.0.53",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.5.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/eslint-plugin": "^1.5.2",
    "bestzip": "^2.2.1",
    "browserslist": "^4.28.1",
    "browserslist-to-esbuild": "^2.1.1",
    "concurrently": "^9.2.1",
    "cypress": "^15.8.1",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "source-map-explorer": "^2.5.2",
    "sst": "^3.17.23",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17"
  },
  "packageManager": "yarn@4.1.0"
}