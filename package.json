{
  "name": "flock",
  "type": "module",
  "version": "1.0.0",
  "description": "Flock app and vault API",
  "author": "David Adams",
  "license": "MIT",
  "private": true,
  "scripts": {
    "analyse": "source-map-explorer 'dist/static/js/*.js'",
    "build": "tsc && vite build",
    "build:vault": "esbuild src/vault/index.ts --outfile=dist/vault/lambda.js --bundle --minify --platform=node --target=node22 --external:\"@aws-sdk/*\" && cd dist/vault && bestzip lambda.zip lambda.js",
    "deploy:app": "aws s3 sync dist/ s3://$APP_BUCKET/ --delete --cache-control max-age=$CACHE_MAX_AGE",
    "dev": "vite",
    "dev:vault": "tsx watch src/vault/api/runServer.ts",
    "initdb": "tsx src/vault/drivers/init.ts",
    "lint": "eslint src/ --ext .ts,.tsx",
    "postinstall": "patch-package",
    "start": "docker compose up -d api && yarn dev",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.513.0",
    "@aws-sdk/core": "^3.513.0",
    "@aws-sdk/lib-dynamodb": "^3.513.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fastify/auth": "^5.0.3",
    "@fastify/aws-lambda": "^6.1.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/type-provider-typebox": "^6.1.0",
    "@loadable/component": "^5.15.3",
    "@mui/icons-material": "^5.15.0",
    "@mui/lab": "^5.0.0-alpha.156",
    "@mui/material": "^5.15.0",
    "@mui/types": "^7.2.11",
    "@mui/x-date-pickers": "^6.18.4",
    "@reduxjs/toolkit": "^2.0.1",
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@types/loadable__component": "^5.13.10",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "18.2.17",
    "@types/react-virtualized": "^9.22.3",
    "@types/zxcvbn": "^4.4.1",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.13.2",
    "csv-parse": "^5.5.5",
    "cypress": "^13.6.1",
    "cypress-file-upload": "^5.0.8",
    "date-fns": "^2.30.0",
    "debounce": "^2.0.0",
    "esbuild": "^0.27.0",
    "fastify": "^5.6.2",
    "firebase": "^10.7.1",
    "firebase-admin": "^10.0.0",
    "happy-dom": "^20.0.10",
    "js-file-download": "^0.4.12",
    "markdown-to-jsx": "^7.3.2",
    "match-sorter": "^6.3.1",
    "mui-file-dropzone": "^4.0.2",
    "nodemon": "^2.0.7",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-move": "^3.0.4",
    "react-hotkeys": "^2.0.0",
    "react-icons": "^4.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "react-virtualized": "^9.22.5",
    "react-window": "^2.2.3",
    "redux": "^5.0.0",
    "redux-thunk": "^3.1.0",
    "serve": "^14.2.1",
    "tsx": "^4.20.6",
    "typebox": "^1.0.53",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "uuid": "^9.0.1",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.2.2",
    "winston": "^3.11.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/debounce": "^1.2.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/eslint-plugin": "^1.4.2",
    "bestzip": "^2.2.1",
    "browserslist-to-esbuild": "^1.2.0",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "source-map-explorer": "^2.5.2",
    "vitest": "^4.0.13"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17"
  },
  "packageManager": "yarn@4.1.0"
}
